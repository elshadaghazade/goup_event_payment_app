{
  "name": "medial-platform-gateway",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "preview": "next build && next start -p 3001",
    "typecheck": "tsc --noEmit",
    "doc:gen": "typedoc",
    "dbml:doc": "dbdocs build ./dbml/database.dbml --project GOUP_EVENT_APP",
    "migrate": "npm run migrate:dev && npm run migrate:test",
    "migrate:dev": "dotenv -e .env -- npx prisma migrate dev --name $(date +\"%Y%m%d%H%M\")",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name $(date +\"%Y%m%d%H%M\")",
    "seed": "npm run seed:dev && npm run seed:test",
    "seed:dev": "dotenv -e .env -- ts-node seed.ts",
    "seed:test": "dotenv -e .env.test -- ts-node seed.ts",
    "docker": "docker-compose stop && docker-compose rm -f && docker-compose up -d && docker volume prune -f && rm -rf ./prisma/migrations",
    "test": "dotenv -e .env.test -- npx jest --detectOpenHandles --verbose"
  },
  "dependencies": {
    "@aws-sdk/client-backupstorage": "^3.540.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/lib-storage": "^3.540.0",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/skip-nav": "^2.1.0",
    "@chakra-ui/system": "^2.6.2",
    "@chakra-ui/theme-tools": "^2.1.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/inter": "^4.5.15",
    "@prisma/client": "5.11.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@saas-ui/auth": "^3.0.6",
    "@saas-ui/react": "^2.7.2",
    "@svgr/webpack": "^7.0.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.4.1",
    "framer-motion": "^10.18.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "0.260.0",
    "next": "^14.1.4",
    "next-seo": "^6.5.0",
    "next-swagger-doc": "^0.4.0",
    "next-themes": "^0.2.1",
    "openapi-types": "^12.1.3",
    "prisma-redis-middleware": "^4.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-remove-scroll": "^2.5.9",
    "request": "^2.79.0",
    "sharp": "^0.32.3",
    "swagger-ui-react": "^5.1.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@faker-js/faker": "^8.4.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/next-auth": "^3.15.0",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.14",
    "dbdocs": "^0.9.1",
    "eslint": "^8.44.0",
    "eslint-config-next": "13.4.10",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "faker": "^6.6.6",
    "form-data": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "next-test-api-route-handler": "^4.0.5",
    "node-mocks-http": "^1.14.1",
    "object-sizeof": "^2.6.3",
    "postcss": "^8.4.26",
    "prettier": "^2.8.8",
    "prisma": "^5.11.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.0",
    "typescript": "^5.1.6"
  }
}
